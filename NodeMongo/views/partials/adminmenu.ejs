			<!-- #section:basics/sidebar -->
			<div id="sidebar" class="sidebar responsive" ng-controller="menucontroller">
				<script type="text/javascript">
					try{ace.settings.check('sidebar' , 'fixed')}catch(e){}
				</script>
				<ul ng-repeat = "menu in menuitems" class="{{menu.ulclass}}">
		                    <!-- Hidden li included to remove active class from about link when scrolled up past about section -->		 
							
		                    <li ng-repeat="listitem in menu.listitems" class="{{listitem.li.class}}">
								
		                        <a is-active-nav class="{{listitem.li.aclass}}" ng-if="listitem.li.href"  href="{{listitem.li.href}}">
									<i class="{{listitem.li.iconclass}}"></i>
									<span class="menu-text"> {{listitem.li.label}} </span>
								</a>
								<a class="{{listitem.li.aclass}}" ng-if="!listitem.li.href" >
									<i class="{{listitem.li.iconclass}}"></i>
									<span class="menu-text"> {{listitem.li.label}} </span>
								</a>
								<b class="arrow"></b>
								<ul class="{{listitem.submenu.ulclass}}" ng-if="listitem.submenu">
									<li ng-repeat="submenuitem in listitem.submenu.listitems" class="{{submenuitem.class}}">
										<a is-active-nav class="{{submenuitem.aclass}}" href="{{submenuitem.href}}">
											<i class="{{submenuitem.iconclass}}"></i>
											{{submenuitem.label}}
										</a>

										<b class="arrow"></b>
									</li>
								</ul>	
								
		                    </li>
		        </ul>
				
	
				<!-- #section:basics/sidebar.layout.minimize -->
				<div class="sidebar-toggle sidebar-collapse" id="sidebar-collapse">
					<i class="ace-icon fa fa-angle-double-left" data-icon1="ace-icon fa fa-angle-double-left" data-icon2="ace-icon fa fa-angle-double-right"></i>
				</div>

				<!-- /section:basics/sidebar.layout.minimize -->
				<script type="text/javascript">
					try{ace.settings.check('sidebar' , 'collapsed')}catch(e){}
					
					
				</script>
			</div>
			<!-- /section:basics/sidebar -->
